{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --port 4000",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "TS_NODE_PROJECT=cypress/tsconfig.json cypress run --spec 'cypress/e2e/**/*.cy.ts'",
    "test:e2e:open": "TS_NODE_PROJECT=cypress/tsconfig.json cypress open --e2e",
    "test:e2e:chrome": "TS_NODE_PROJECT=cypress/tsconfig.json cypress run --browser chrome --headed --spec 'cypress/e2e/**/*.cy.ts'",
    "test:e2e:firefox": "TS_NODE_PROJECT=cypress/tsconfig.json cypress run --browser firefox --headed --spec 'cypress/e2e/**/*.cy.ts'",
    "test:e2e:edge": "TS_NODE_PROJECT=cypress/tsconfig.json cypress run --browser edge --headed --spec 'cypress/e2e/**/*.cy.ts'",
    "test:e2e:electron": "TS_NODE_PROJECT=cypress/tsconfig.json cypress run --browser electron --headed --spec 'cypress/e2e/**/*.cy.ts'",
    "test:e2e:chrome:headless": "TS_NODE_PROJECT=cypress/tsconfig.json cypress run --browser chrome --spec 'cypress/e2e/**/*.cy.ts'",
    "test:e2e:firefox:headless": "TS_NODE_PROJECT=cypress/tsconfig.json cypress run --browser firefox --spec 'cypress/e2e/**/*.cy.ts'",
    "test:e2e:edge:headless": "TS_NODE_PROJECT=cypress/tsconfig.json cypress run --browser edge --spec 'cypress/e2e/**/*.cy.ts'",
    "test:e2e:electron:headless": "TS_NODE_PROJECT=cypress/tsconfig.json cypress run --browser electron --spec 'cypress/e2e/**/*.cy.ts'"
  },
  "dependencies": {
    "@nuxt/eslint": "1.6.0",
    "@nuxt/image": "1.0.0",
    "@nuxt/test-utils": "3.19.2",
    "@nuxt/ui": "3.2.0",
    "@pinia/nuxt": "0.11.2",
    "eslint": "9.31.0",
    "lodash": "4.17.21",
    "nuxt": "3.17.7",
    "typescript": "5.8.3"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "6.14.0",
    "@types/cypress": "0.1.6",
    "@types/jest": "30.0.0",
    "@types/lodash": "4.17.20",
    "@vue/tsconfig": "0.7.0",
    "autoprefixer": "10.4.21",
    "cypress": "13.6.0",
    "postcss": "8.5.6",
    "tailwindcss": "3.4.3",
    "ts-node": "10.9.1"
  }
}
